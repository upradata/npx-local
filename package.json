{
	"name": "local-installation",
	"version": "1.0.0",
	"description": "Npm Local Packages Install",
	"main": "lib",
	"module": "lib-esm",
	"scripts": {
		"git:push": "npm version patch && git push && git push --tags",
		"npm:publish": "npm publish --access public",
		"postinstall": "npx install-local",
		"clean": "shx rm -r dist || true",
		"prebuild": "npm run clean",
		"postbuild": "npm run all-test",
		"build": "yarn run clean && tsc",
		"build2": "yarn run clean && tsc && tsc -m es6 --outDir lib-esm",
		"all-test": "npm run test && npm run e2e",
		"test": "jest --config jest-ut.config.js",
		"e2e": "jest --config jest-e2e.config.js",
		"renamer": "renamer -regex --find '\\.js$' --replace '.mjs' './lib-esm/**/*.js'"
	},
	"author": "Thomas Milotti <thomas.milotti@gmail.com> (https://github.com/milottit)",
	"license": "MIT",
	"files": [
		"lib",
		"lib-esm"
	],
	"dependencies": {
		"@types/yargs": "^15.0.5",
		"babel-core": "^6.26.3",
		"babel-loader": "^8.1.0",
		"babel-preset-env": "^1.7.0",
		"camelcase": "^6.0.0",
		"colors": "^1.4.0",
		"find-up": "^4.1.0",
		"fs-extra": "^9.0.0",
		"glob": "^7.1.6",
		"glob-move": "^1.0.1",
		"gulp": "^4.0.2",
		"gulp-typescript": "^6.0.0-alpha.1",
		"json5": "^2.1.3",
		"mv": "^2.1.1",
		"node-run-cmd": "^1.0.1",
		"rimraf": "^3.0.2",
		"rxjs": "^6.5.5",
		"stickies": "file:../Stickies",
		"yargs": "^15.3.1"
	},
	"devDependencies": {
		"@schemastore/package": "0.0.5",
		"@types/fs-extra": "^9.0.1",
		"@types/jest": "^25.2.3",
		"@types/node": "^14.0.5",
		"@types/rimraf": "^3.0.0",
		"jest": "^26.0.1",
		"jest-expect-message": "^1.0.2",
		"node-watch": "^0.6.4",
		"ts-jest": "^26.0.0",
		"ts-node": "^8.10.1"
	},
	"local": {
		"dependencies": {
			"@upradata/node-util": "link:/home/milottit/Libraries/Util/node-util@2.0.3",
			"@upradata/util": "link:/home/milottit/Libraries/Util/util@1.0.24"
		}
	}
}
