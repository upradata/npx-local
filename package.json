{
    "name": "@upradata/npmlocal",
    "version": "1.0.0",
    "description": "Npm Local Packages Install",
    "main": "lib-esm",
    "types": "lib-esm",
    "module": "lib-esm",
    "files": [
        "lib-esm"
    ],
    "scripts": {
        "git:push": "npm version patch && git push && git push --tags",
        "npm:publish": "npm publish --access public",
        "clean": "shx rm -r dist || true",
        "prebuild": "npm run clean",
        "postbuild": "npm run all-test",
        "build": "yarn run clean && tsc",
        "build2": "yarn run clean && tsc && tsc -m es6 --outDir lib-esm",
        "all-test": "npm run test && npm run e2e",
        "test": "jest --config jest-ut.config.js",
        "e2e": "jest --config jest-e2e.config.js",
        "renamer": "renamer -regex --find '\\.js$' --replace '.mjs' './lib-esm/**/*.js'"
    },
    "author": "Thomas Milotti <thomas.milotti@gmail.com> (https://github.com/milottit)",
    "license": "MIT",
    "dependencies": {
        "@types/yargs": "^15.0.9",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.2.1",
        "babel-preset-env": "^1.7.0",
        "camelcase": "^6.2.0",
        "colors": "^1.4.0",
        "find-up": "^5.0.0",
        "fs-extra": "^9.0.1",
        "get-caller-file": "^2.0.5",
        "glob": "^7.1.6",
        "glob-move": "^1.0.1",
        "gulp": "^4.0.2",
        "gulp-typescript": "^6.0.0-alpha.1",
        "json5": "^2.1.3",
        "mock-require": "^3.0.3",
        "mv": "^2.1.1",
        "node-run-cmd": "^1.0.1",
        "normalize-path": "^3.0.0",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.3",
        "stickies": "link:../Stickies",
        "yargs": "^16.1.0"
    },
    "devDependencies": {
        "@schemastore/package": "0.0.6",
        "@types/fs-extra": "^9.0.4",
        "@types/jest": "^26.0.15",
        "@types/mock-require": "^2.0.0",
        "@types/node": "^14.14.7",
        "@types/normalize-path": "^3.0.0",
        "@types/rimraf": "^3.0.0",
        "concurrently": "^5.3.0",
        "jest": "^26.6.3",
        "jest-expect-message": "^1.0.2",
        "node-watch": "^0.7.0",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.0.0"
    },
    "local": {
        "dependencies": {
            "@upradata/node-util": "link:/home/milottit/Libraries/Util/node-util@2.0.3",
